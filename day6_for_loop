//code1:

let bag ="";
for(i=1;i<=20;i++){
  if(i%2==1){
    bag = bag + i + " ";
}
}
  console.log(bag);

//code2:
let bag ="";
for(i=1;i<=4;i++){
  bag = bag + i +"-*";
}
console.log(bag);

//code3:
let sum =0;
for(i=1;i<=15;i++){
  if(i%2==0||i%5==0){
    sum = sum + i;
  }
}
console.log(sum);

//code4:
let sum =0;
let count =0;
for(i=1;i<=50;i++){
  if(i%2==0){
  sum = sum + i;
  count++;
}
}
console.log(sum/count);
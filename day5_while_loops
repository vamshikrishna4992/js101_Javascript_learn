//code1:
let i=10;
let j=20;
while(i<=j){
  console.log(i);
  i++;
}

//code2:
let i=0;
let j=20;
while(i<=j){
  if(i%2==1){
    console.log(i);
  }
  i++;
}

//code3:
let i=0;
let j=10;
let sum =0;
while(i<=j){
  if(i%3==0){
    sum = sum + i;
  }
  i++;
}
console.log(sum);

//code4:
let i=1;
let sum =0;
let count = 0;
while(i<=100){
  if(i%2==0){
    sum =  sum +i;
    count++;
  }
  i++;
}
console.log(sum/count);

